@{
    //ViewBag.Title = "Calendar";
}
<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Message</h3>
<center>
    <p>Calendar</p>
    <iframe src="https://calendar.google.com/calendar/embed?src=9abu118f0uj7rft4lr1bvndmec%40group.calendar.google.com&ctz=Asia%2FKuala_Lumpur" style="border: 0" width="800" height="600" frameborder="0" scrolling="no">Click here to view calendar</iframe>
    <hr />
    <table>
        <tr>
            <th>List of Events</th>
            <th>Book Event</th>
        </tr>
        <tr>
            <td>Event</td>
            <td> <button id="btn1" onclick="addOne(this)"type="button">I'm in!</button> </td>
            <td>
                <p><span id="one">0</span></p>
                <script type="text/javascript">

                   
                    function addOne(btn1) {
                        var foo = document.getElementById('one').innerHTML;
                        foo++;
                        document.getElementById('one').innerHTML = foo;
                        document.getElementById(btn1.id).disabled = true;
                    }

                </script>
            </td>
        </tr>
        <tr>
            <td>Event</td>
            <td> <button id="btn2" onclick="addTwo(this)"type="button">I'm in!</button> </td>
            <td>
                <p><span id="two">0</span></p>
                <script type="text/javascript">


                    function addTwo(btn2) {
                        var foo = document.getElementById('two').innerHTML;
                        foo++;
                        document.getElementById('two').innerHTML = foo;
                        document.getElementById(btn2.id).disabled = true;
                    }

                </script>
            </td>
        </tr>
    </table>
    </center>

